    "host": "http://localhost:8000",
    "basePath": "/api/v1",
    "paths": {
        "/auth/signup": {
            "post": {
                "description": "通过邮箱密码注册账号",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "账号注册",
                "parameters": [
                    {
                        "description": "测试",
                        "name": "test",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    },
                    {
                        "description": "邮箱账号",
                        "name": "email",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    },
                    {
                        "description": "账号密码",
                        "name": "accountPass",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    },
                    {
                        "description": "确认密码",
                        "name": "confirmPass",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{\"status\":200, \"code\": 2000001, msg:\"请求处理成功\"}",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "{\"status\":400, \"code\": 4000003, msg:\"注册密码不匹配\"}",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "{\"status\":500, \"code\": 5000001, msg:\"服务器内部错误\"}",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}